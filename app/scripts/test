#!/usr/bin/env bash
### Usage:
###   test
### Options:
###   --help
###
### hyperdrive test script for playing with bash stuff

#--HEAD--
SCRIPT="$(readlink -e -- "${0}")"
SCRIPT_NAME="$(basename "${SCRIPT}")"
SCRIPT_DIR="$(dirname "${SCRIPT}")"
source "${SCRIPT_DIR}/main.source"
#--/HEAD--

ACTIVE_ORDERS="$(orders_list | awk -F/ '{print $(NF-1)}')"
ORDER_REPOS="$(local_order_repos | awk -F/ '{print $(NF)}')"

grep -Fvxf <(echo -n "${ACTIVE_ORDERS}") <(echo -n "${ORDER_REPOS}") | sed -e "s/^/${HYPERDRIVE_ORDER_REPOS//\//\\\/}\//g"
